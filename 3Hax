if game.CoreGui:FindFirstChild("3Hax") then
	
	game.TweenService:Create(game.CoreGui:FindFirstChild("3Hax").Menu,TweenInfo.new(0.5),{BackgroundTransparency = 1,Position = UDim2.new(0,0,0.962,0)}):Play()
	game.TweenService:Create(game.CoreGui:FindFirstChild("3Hax").Menu.Toggle,TweenInfo.new(0.5),{Position = UDim2.new(0.475, 0, 0.5, 0)}):Play()
	game.TweenService:Create(game.CoreGui:FindFirstChild("3Hax").Menu.Toggle.Icon,TweenInfo.new(0.5),{ImageTransparency = 1}):Play()
	
	wait(0.5)
	
	game.CoreGui:FindFirstChild("3Hax"):Destroy()
	
end

